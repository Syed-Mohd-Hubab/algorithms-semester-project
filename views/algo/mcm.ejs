<%- include('../partials/header') %>
    <div class="container">
        <div class="row">
            <section id="appointment" data-stellar-background-ratio="3">
                <h2>Matrix Chain Multiplication Problem</h2>
                <div class="col-xs-12">
                    <div class="col-xs-12">
                        <% for(let i=0 ; i<10 ; i++){ %> 
                            <p><%=i+1%>)<%=array[i]%></p>
                            <% if(answer[i].length != -1){ %> 
                            <strong>Minimum Operations:  <%=answer[i].length%></strong> <br>
                            <% }else{ %>
                            <strong>Minimum Operations:  Answer too big that could not fit the maximum supported size of integer</strong> <br>
                            <% } %>
                            <p style="overflow: auto; font-weight: bold; color: #333;">Bracketting: <%=answer[i].bracket%></p> <br>
                        <% } %>
                    </div>
                </div>
            </section>
        </div>
   </div>
<%- include('../partials/footer') %>