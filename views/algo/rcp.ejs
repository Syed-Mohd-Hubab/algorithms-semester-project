<%- include('../partials/header') %>
    <div class="container">
        <div class="row">
            <section id="appointment" data-stellar-background-ratio="3">
                <h2>RodCut Problem</h2>
                <div class="col-xs-12">
                    <h4>Rod Length:<%=rodlength%></h4>
                    <div class="col-xs-12">
                        <% for(let i=0, j=0 ; i<20 ; i=i+2, j++){ %> 
                            <p><%=j+1%>)<%=array[i]%> <br>
                                <%=array[i+1]%></p>
                            <strong>Max profit: <%=length[j]%></strong> <br><br>
                        <% } %>
                    </div>
                </div>
             </section>
        </div>
   </div>
<%- include('../partials/footer') %>