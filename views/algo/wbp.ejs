<%- include('../partials/header') %>
    <div class="container">
        <div class="row">
            <section id="appointment" data-stellar-background-ratio="3">
                <h2>Word Break Problem</h2>
                <div class="col-xs-12">
                    <h4>String: <%=name%></h4>
                    <div class="col-xs-12">
                        <% for(let i=0 ; i<10 ; i++){ %> 
                            <p style="overflow: auto;"><%=i+1%>)<%=array[i]%></p>
                            <strong>Word can be broken: <%=length[i]%></strong> <br><br>
                        <% } %>
                    </div>
                </div>
             </section>
        </div>
   </div>
<%- include('../partials/footer') %>